    "coveralls": "node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastNick/project-lvl2-s401.git"
  },
  "keywords": [
    "cli",
    "application",
    "parse",
    "format",
    "json",
    "yaml",
    "ini",
    "data"
  ],
  "author": "Nick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fastNick/project-lvl2-s401/issues"
  },
  "homepage": "https://github.com/fastNick/project-lvl2-s401#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.5",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^24.8.0",
    "require-self": "^0.x"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "gendiff-nick": "*",
    "ini": "^1.3.5",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.14"
  },
  "files": [
    "dist"
  ]
