{
  "name": "gendiff-nick",
  "version": "1.2.8",
  "description": "Learn how to create cli applications, parse and format data in json, yaml, ini. Learn to design an application architecture. And also write unit tests.",
  "main": "dist/index.js",
  "bin": {
    "gendiff": "dist/bin/gendiff.js"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublishOnly": "npm run build",
